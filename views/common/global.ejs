<script type="text/javascript" src="<%-STATIC_PATH%>/javascripts/lib/validate.js"></script>
<link type="text/css" href="<%-STATIC_PATH%>/javascripts/lib/tip/tipCss.css" rel="stylesheet" />
<link href="<%-STATIC_PATH%>/javascripts/lib/emTable/emTable.css" rel="stylesheet" type="text/css" />
<link rel="stylesheet" type="text/css" href="<%-STATIC_PATH%>/stylesheets/common.css">
<link rel="stylesheet" type="text/css" href="<%-STATIC_PATH%>/stylesheets/style.css">
